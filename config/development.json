   "action_required":{
      "back":[
        "pid"
      ],
      "click":[
         "selector",
         "pid"
      ],
      "download_image":[
         "selector",
         "dl_file_loc",
         "pid"
      ],
      "enter_text":[
         "selector",
         "text",
         "pid"
      ],
      "forward":[
        "pid"
