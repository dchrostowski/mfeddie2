      "text":"string",
      "user_agent":"string",
      "url":"string",
      "keep_alive":"int",
      "timeout":"int",
      "force":"int",
      "force_selector_type":"string",
      "get_content":"int",
      "return_on_timeout":"int",
      "load_external":"int",
      "load_images":"int",
      "load_css":"int",
      "require_proxy":"int",
      "proxy":"string",
      "allowed":"json",
      "disallowed":"json"
   },
   "action_optional_defaults":{
      "back":{
         "keep_alive":1,
         "timeout": 2000
      },
      "click":{
         "timeout":800,
